#include <iostream>

int main()
{
   std::cout << "Hello world!";
   
   std::cout<<'S';
   std::cout<<"Aqsa";
   
   
  std::cout<<'5';//it prints the numerical value not the number 5.
//   std::cout<<'5.6';
// this will not come because of it contained poin 

   std::cout<<"5.6";
   std::cout<<5;
   std::cout<<5.6;
   
